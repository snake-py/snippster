{
  "name": "snippster",
  "version": "0.1.4",
  "author": "Fabio Batti",
  "description": "Snippster helps to organize your code!",
  "private": true,
  "main": "public/electron.js",
  "repository": "git@github.com:snake-py/snippster.git",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "ace-builds": "^1.4.12",
    "better-sqlite3": "^7.1.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.1",
    "electron-updater": "^4.3.5",
    "image-size": "^0.9.3",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-ace": "^9.2.0",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-resize-panel": "^0.3.5",
    "react-scripts": "4.0.0",
    "react-select": "^3.1.1",
    "react-split": "^2.0.9",
    "react-svg": "^11.1.1",
    "react-textarea-autosize": "^8.3.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "wait-on": "^5.2.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "test": "react-scripts test",
    "test-spectron": "mocha --watch --timeout 10000",
    "react-start": "react-scripts start",
    "rebuild": "electron-rebuild -f -w better-sqlit3",
    "postinstall": "electron-builder install-app-deps",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm run react-build && electron-builder --publish=always",
    "build": "npm run react-build && npm run electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "build": {
    "productName": "Snippster",
    "appId": "com.electron.Snippster",
    "publish": {
      "provider": "spaces",
      "name": "snippster",
      "region": "fra1",
      "acl": "public-read"
    },
    "directories": {
      "buildResources": "C:/Users/vabis/Desktop/Playground/projects/snippster",
      "output": "C:/Users/vabis/Desktop/Playground/projects/snippster/dist",
      "app": "C:/Users/vabis/Desktop/Playground/projects/snippster"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "assets/mac/icon.icns"
    },
    "linux": {
      "icon": "assets/png/icon.png"
    },
    "win": {
      "icon": "assets/win/icon.ico"
    },
    "files": [
      "!electron-builder.env",
      "!/test"
    ],
    "extends": null
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.7",
    "devtron": "^1.4.0",
    "electron": "^10.1.5",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.2",
    "electron-reloader": "^1.0.1",
    "mocha": "^8.2.1",
    "redux-mock-store": "^1.5.4",
    "spectron": "^12.0.0"
  }
}